<article id="home" class="container">
  <div class="row">
    <div class="col s12 m11 l11">
      <ul class="collapsible" data-collapsible="expendable">
        <% @post.reverse.each do |post| %>
			  <li class="active">
			    <div class="collapsible-header active"><strong><%= post.title %></strong></div>
			    <div class="collapsible-body" style="display: block;">
			      <%= raw post.content %>
			    </div>
			  </li>
			  <% end %>
			</ul>
    </div>
  </div>
</article>