<%= form_for([@assignment, @submission]) do |f| %>
    <%= hidden_field_tag 'user_id', @user.id %>
    <%= hidden_field_tag 'assignment_id', @assignment.id %>
  <div class="field">
    <%= f.label :c9url %><br>
    <%= f.text_field :c9url %>
  </div>
  <div class="field">
    <%= f.label :giturl %><br>
    <%= f.text_field :giturl %>
  </div>
  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </div>
  
  <div class="action">
    <%= f.submit %>
  </div>
<% end %>